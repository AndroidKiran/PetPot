<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_margin="@dimen/grid_1">

    <include
        android:id="@+id/cv_0"
        layout="@layout/item_load_search"
        android:layout_width="match_parent"
        android:layout_height="240dp"
        app:layout_constraintBottom_toTopOf="@id/cv_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <include
        android:id="@+id/cv_1"
        layout="@layout/item_load_search"
        android:layout_width="@dimen/zero"
        android:layout_height="160dp"
        android:layout_marginTop="@dimen/grid_0_4"
        app:layout_constraintBottom_toTopOf="@id/cv_4"
        app:layout_constraintEnd_toStartOf="@id/cv_2"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cv_0" />

    <include
        android:id="@+id/cv_2"
        layout="@layout/item_load_search"
        android:layout_width="@dimen/zero"
        android:layout_height="160dp"
        android:layout_marginStart="@dimen/grid_0_2"
        android:layout_marginEnd="@dimen/grid_0_2"
        app:layout_constraintBottom_toBottomOf="@id/cv_1"
        app:layout_constraintEnd_toStartOf="@id/cv_3"
        app:layout_constraintStart_toEndOf="@id/cv_1"
        app:layout_constraintTop_toTopOf="@id/cv_1" />

    <include
        android:id="@+id/cv_3"
        layout="@layout/item_load_search"
        android:layout_width="@dimen/zero"
        android:layout_height="160dp"
        android:layout_marginStart="@dimen/grid_0_2"
        app:layout_constraintBottom_toBottomOf="@id/cv_1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/cv_2"
        app:layout_constraintTop_toTopOf="@id/cv_1" />

    <include
        android:id="@+id/cv_4"
        layout="@layout/item_load_search"
        android:layout_width="@dimen/zero"
        android:layout_height="160dp"
        android:layout_marginTop="@dimen/grid_0_4"
        app:layout_constraintEnd_toEndOf="@id/cv_1"
        app:layout_constraintStart_toStartOf="@id/cv_1"
        app:layout_constraintTop_toBottomOf="@id/cv_1" />

    <include
        android:id="@+id/cv_5"
        layout="@layout/item_load_search"
        android:layout_width="@dimen/zero"
        android:layout_height="160dp"
        app:layout_constraintBottom_toBottomOf="@id/cv_4"
        app:layout_constraintEnd_toEndOf="@id/cv_2"
        app:layout_constraintStart_toStartOf="@id/cv_2"
        app:layout_constraintTop_toTopOf="@id/cv_4" />

    <include
        android:id="@+id/cv_6"
        layout="@layout/item_load_search"
        android:layout_width="@dimen/zero"
        android:layout_height="160dp"
        app:layout_constraintBottom_toBottomOf="@id/cv_4"
        app:layout_constraintEnd_toEndOf="@id/cv_3"
        app:layout_constraintStart_toStartOf="@id/cv_3"
        app:layout_constraintTop_toTopOf="@id/cv_4" />

</androidx.constraintlayout.widget.ConstraintLayout>